{
    "root": "Dataset",
    "types": {
        "Dataset": {
            "fields": {
                "@type": {"remove": true},
                "accessLevel": {"remove": true},
                "publisher": {"remove": true},
                "identifier": {"remove": true},
                "theme": {"remove": true},

                "title": {},
                "description": {"map": "notes", "validators": ["dga_tsm_parse_description"]},
                "temporal_coverage_from": {"default_from": "modified", "validators": ["dga_tsm_parse_issued_date"]},
                "modified": {"map": "remote_last_updated", "validators": ["dga_tsm_parse_issued_date"]},
                "keyword": {"validators": ["dga_tsm_words_into_tags"], "map": "tags"},

                "license_id": {"default": "other"},
                "rights": {"map": "license_id", "validators": ["dga_tsm_southern_grampians_parse_license"]},

                "landingPage": {"map": "name", "validators": ["dga_tsm_melbourne_from_url"]},
                "distribution": {
                    "type": "Resource",
                    "multiple": true,
                    "map": "resources"
                },

                "contact_point": {"default": "unknown"},
                "language": {"map": "language", "validators": ["dga_tsm_southern_grampians_parse_language"]}
            }
        },
        "Resource": {
            "fields": {
                "accessURL": {"map": "url"},
                "downloadURL": {"map": "url"},
                "mediaType": {"map": "mimetype"},
                "format": {"map": "format"},
                "@type": {"remove": true}
            }
        }
    }
}
