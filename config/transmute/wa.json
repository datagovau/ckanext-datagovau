{
    "root": "Dataset",
    "types": {
        "Dataset": {
            "fields": {
                "extras": {"remove": true, "weight": 10},
                "notes": {"validators": ["dga_tsm_parse_description"]},
                "data_state": {"remove": "true"},
                "metadata_created": {},
                "license_id": {"default": "notspecified"},

                "spatial": {"map": "spatial_coverage"},
                "data_temporal_extent_begin": {"default_from": "metadata_created", "map": "temporal_coverage_from"},
                "data_temporal_extent_end": {"map": "temporal_coverage_to"},
                "author_email": {"map": "contact_point"},
                "remote_last_updated": {"default_from": "metadata_modified", "validators": [
                    "dga_tsm_parse_date", "tsm_to_string"
                    ]
                },
                "original_name": {"default_from": "name"}
            }
        },
        "Resource": {
            "fields": {
            }
        }
    }
}
